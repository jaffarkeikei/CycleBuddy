{
  "name": "cyclebuddy-stellar",
  "version": "1.0.0",
  "description": "A Web3-powered menstrual health companion built on Stellar and Bahamut",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "vite build",
    "vercel-build": "npm run build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "build:contracts": "make build",
    "test:contracts": "make test",
    "deploy:contracts": "make deploy",
    "clean": "make clean",
    "deploy:testnet": "make deploy-save",
    "deploy:mainnet": "stellar-cli deploy --network mainnet",
    "optimize:contracts": "make optimize",
    "generate:bindings": "make bindings",
    "test:donation": "cd contracts/donation && cargo test",
    "test:data-sharing": "cd contracts/data-sharing && cargo test",
    "test:rewards": "cd contracts/rewards && cargo test",
    "test:zk-validation": "cd contracts/zk-validation && cargo test",
    "test:data-marketplace": "cd contracts/data-marketplace && cargo test",
    "test:health-alerts": "cd contracts/health-alerts && cargo test",
    "bahamut:compile": "cd contracts/bahamut && npx hardhat compile",
    "bahamut:test": "cd contracts/bahamut && npx hardhat test",
    "bahamut:deploy:testnet": "cd contracts/bahamut && npx hardhat run scripts/deploy.js --network bahamut",
    "bahamut:deploy:mainnet": "cd contracts/bahamut && npx hardhat run scripts/deploy.js --network bahamutMainnet",
    "bahamut:verify": "cd contracts/bahamut && npx hardhat verify --network bahamut"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^1.7.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@soroban-react/core": "*",
    "@soroban-react/freighter": "*",
    "@stellar/freighter-api": "^1.7.1",
    "@tanstack/react-query": "^4.32.6",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "ethers": "^5.7.2",
    "framer-motion": "^10.15.1",
    "ipfs-http-client": "^60.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "soroban-client": "^1.0.0",
    "stellar-sdk": "^11.0.0",
    "tailwindcss": "^3.3.3",
    "zod": "^3.21.4",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "^4.9.3",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "chai": "^4.3.7",
    "dotenv": "^16.5.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "hardhat": "^2.24.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.8.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/cyclebuddy-stellar.git"
  },
  "keywords": [
    "stellar",
    "web3",
    "blockchain",
    "health",
    "privacy",
    "education",
    "bahamut",
    "evm"
  ],
  "author": "CycleBuddy Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/cyclebuddy-stellar/issues"
  },
  "homepage": "https://github.com/yourusername/cyclebuddy-stellar#readme"
}
